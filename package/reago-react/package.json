{
  "name": "reago-react",
  "author": "Areven, Sebastian Nowak",
  "license": "MIT",
  "version": "0.3.0",
  "description": "Official React bindings for the Reago atomic state manager",
  "keywords": [
    "reago",
    "react",
    "official"
  ],
  "homepage": "https://reago.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/areven/reago.git",
    "directory": "package/reago-react"
  },
  "bugs": {
    "url": "https://github.com/areven/reago/issues"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "readme.md",
    "license.md",
    "changelog.md",
    "./dist/**/*",
    "!**/*.tsbuildinfo"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "raw": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "raw": "./src/index.ts",
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest"
  },
  "peerDependencies": {
    "@types/react": ">=17.0.0",
    "react": ">=17.0.0",
    "reago": "^0.3.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "18.3.12",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "vitest-browser-react": "0.2.0"
  }
}
