{
  "name": "reago-vue",
  "author": "Sebastian Daniel Nowak",
  "license": "MIT",
  "version": "0.0.1",
  "description": "Vue bindings for the `reago` state manager",
  "keywords": [
    "reago",
    "vue",
    "official"
  ],
  "homepage": "https://reago.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-nowak/reago.git",
    "directory": "package/reago-vue"
  },
  "bugs": {
    "url": "https://github.com/sebastian-nowak/reago/issues"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "readme.md",
    "changelog.md",
    "./dist/**/*",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "import": {
        "raw": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "raw": "./src/index.ts",
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest"
  },
  "peerDependencies": {
    "reago": "^0.1.3",
    "vue": ">=3.2.0"
  },
  "devDependencies": {
    "vitest-browser-vue": "0.0.1",
    "vue": "3.5.12"
  }
}
